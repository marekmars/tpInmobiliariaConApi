<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.contrato.contratoDetalle.ContratoDetalleFragment"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/background"
    android:id="@+id/flContratoDealle">

    <androidx.cardview.widget.CardView
        android:id="@+id/cvContratoDealle"
        android:layout_width="379dp"
        android:layout_height="647dp"
        app:cardCornerRadius="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/clContratodetalle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/top_bar">


            <LinearLayout
                android:id="@+id/linearContratoDealle"
                android:layout_width="218dp"
                android:layout_height="437dp"
                android:layout_marginEnd="1dp"
                android:layout_marginBottom="3dp"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/linearContratoDealle2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.077">

                <TextView
                    android:id="@+id/tvCodigoLabelCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginStart="15sp"
                    android:layout_marginTop="20sp"
                    android:layout_marginBottom="30sp"
                    android:text="Código"
                    android:textColor="@android:color/system_accent2_50"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tvFechaInicioLabelCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginStart="15sp"
                    android:layout_marginBottom="30sp"
                    android:text="Fecha de Inicio"
                    android:textColor="@android:color/system_accent2_50"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tvFechaFinLabelCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginStart="15sp"
                    android:layout_marginBottom="30sp"
                    android:text="Fecha Fin"
                    android:textColor="@android:color/system_accent2_50"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tvMontoLabelCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginStart="15sp"
                    android:layout_marginBottom="30sp"
                    android:text="Monto"
                    android:textColor="@android:color/system_accent2_50"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tvInquilinoLabelCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginStart="15sp"
                    android:layout_marginBottom="30sp"
                    android:text="Inquilino"
                    android:textColor="@android:color/system_accent2_50"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tvInmuebleLabelCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginStart="15sp"
                    android:layout_marginBottom="30sp"
                    android:text="Inmueble"
                    android:textColor="@android:color/system_accent2_50"
                    android:textSize="20sp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearContratoDealle2"
                android:layout_width="162dp"
                android:layout_height="437dp"
                android:gravity="start"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.076">

                <TextView
                    android:id="@+id/tvCodigoCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginTop="20sp"
                    android:layout_marginEnd="15sp"
                    android:layout_marginBottom="30sp"
                    android:gravity="center_vertical|end"
                    android:shadowColor="@color/material_dynamic_neutral_variant90"
                    android:text="Código"
                    android:textColor="@android:color/holo_blue_light"
                    android:textSize="15sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tvFechaInicioCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginEnd="15sp"
                    android:layout_marginBottom="30sp"
                    android:gravity="center_vertical|end"
                    android:shadowColor="@color/material_dynamic_neutral_variant90"
                    android:text="Fecha de Inicio"
                    android:textColor="@android:color/holo_blue_light"
                    android:textSize="15sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tvFechaFinCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginEnd="15sp"
                    android:layout_marginBottom="30sp"
                    android:gravity="center_vertical|end"
                    android:shadowColor="@color/material_dynamic_neutral_variant90"
                    android:text="Fecha Fin"
                    android:textColor="@android:color/holo_blue_light"
                    android:textSize="15sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tvMontoCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginEnd="15sp"
                    android:layout_marginBottom="30sp"
                    android:gravity="center_vertical|end"
                    android:shadowColor="@color/material_dynamic_neutral_variant90"
                    android:text="Monto"
                    android:textColor="@android:color/holo_blue_light"
                    android:textSize="15sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tvInquilinoCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginEnd="15sp"
                    android:layout_marginBottom="30sp"
                    android:gravity="center_vertical|end"
                    android:shadowColor="@color/material_dynamic_neutral_variant90"
                    android:text="Inquilino"
                    android:textColor="@android:color/holo_blue_light"
                    android:textSize="15sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tvInmuebleCD"
                    android:layout_width="match_parent"
                    android:layout_height="35sp"
                    android:layout_marginEnd="15sp"
                    android:layout_marginBottom="30sp"
                    android:gravity="center_vertical|end"
                    android:shadowColor="@color/material_dynamic_neutral_variant90"
                    android:text="Inmueble"
                    android:textColor="@android:color/holo_blue_light"
                    android:textSize="15sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <Button
                android:id="@+id/btnPagos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="65dp"
                android:text="Ver Pagos"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.54"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearContratoDealle"
                app:layout_constraintVertical_bias="0.0" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>